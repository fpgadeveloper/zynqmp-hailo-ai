[clock]

freqHz=250000000:v_multi_scaler_1.ap_clk

[connectivity]

sp=v_multi_scaler_1.m_axi_mm_video:HPC1

[advanced]
misc=:solution_name=link

param=compiler.addOutputTypes=sd_card
#Following switch will ignore the timing violations during xclbin creation
#param=compiler.skipTimingCheckAndFrequencyScaling=1

[vivado]
prop=run.impl_1.STEPS.OPT_DESIGN.ARGS.DIRECTIVE=ExploreArea
prop=run.impl_1.STEPS.PLACE_DESIGN.ARGS.DIRECTIVE=Explore
prop=run.impl_1.STEPS.PHYS_OPT_DESIGN.IS_ENABLED=true
prop=run.impl_1.STEPS.PHYS_OPT_DESIGN.ARGS.DIRECTIVE=AggressiveExplore
prop=run.impl_1.STEPS.ROUTE_DESIGN.ARGS.DIRECTIVE=AggressiveExplore

